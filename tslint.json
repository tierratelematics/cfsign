{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "arrow-parens": false,
        "interface-name": false,
        "max-classes-per-file": false,
        "no-string-literal": false,
        "object-literal-shorthand": false,
        "trailing-comma": false,
        "variable-name": [true, "ban-keywords", "allow-leading-underscore"]
      }
}
